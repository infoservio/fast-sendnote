{
    "name": "endurant/mail-manager",
    "description": "Mail Manager",
    "type": "craft-plugin",
    "version": "1.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "mail-manager"
    ],
    "support": {
        "docs": "http://test",
        "issues": "http://test"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "endurant",
            "homepage": "https://endurant.org"
        }
    ],
    "require": {
        "craftcms/cms": "~3.0.0-beta.23",
        "yiisoft/yii2": "^2.0",
        "php-http/curl-client": "^1.7",
        "guzzlehttp/psr7": "^1.4",
        "php-http/guzzle6-adapter": "^1.1.1",
        "mailgun/mailgun-php": "^2.4",
        "postal/postal": "^1.0"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
          "endurant\\mailmanager\\": "src/"
        }
    },
    "extra": {
        "name": "Mail Manager",
        "handle": "mail-manager",
        "schemaVersion": "1.0.0",
        "hasCpSettings": true,
        "hasCpSection": true,
        "changelogUrl": "???",
        "components": {
            "mail": "endurant\\mailmanager\\services\\MailService",
            "template": "endurant\\mailmanager\\services\\TemplateService",
            "changes": "endurant\\mailmanager\\services\\ChangesService",
            "log": "endurant\\mailmanager\\services\\LogService",
            "templateParser": "endurant\\mailmanager\\components\\parser\\TemplateParser",
            "logger": "endurant\\mailmanager\\components\\logger\\Logger"
        },
        "class": "endurant\\mailmanager\\MailManager"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.2"
    }
}
